[sub_resource type="GDScript" id="GDScript_gkphq"]
script/source = "# UI.gd (CanvasLayer) - –°–í–™–†–ó–ê–ù–û –í–°–ò–ß–ö–û!
extends CanvasLayer
class_name GameUI

@onready var pause_ui: Control = $PauseUI
@onready var game_over_ui: Control = $GameOverUI
@onready var score_label: Label = $ScoreLabel
@onready var health_bar: ProgressBar = $HealthBar
@onready var time_label: Label = $TimeLabel
@onready var game_over_score_label: Label = $GameOverUI/ScoreLabel
@onready var ambience_loop: AudioStreamPlayer = $IdleMenuSound

var score: int = 0
var current_health: int = 100

signal resume_game  # –ó–∞ GameScene
signal retry_game
signal quit_game

func _ready():
	# üî• –í–°–ò–ß–ö–ò –ë–£–¢–û–ù–ò –¢–£–ö!
	pause_ui.get_node(\"ContinueButton\").pressed.connect(_on_resume)
	pause_ui.get_node(\"RetryButton\").pressed.connect(_on_pause_retry)
	pause_ui.get_node(\"QuitButton\").pressed.connect(_on_pause_quit)
	
	game_over_ui.get_node(\"RetryButton\").pressed.connect(_on_game_over_retry)
	game_over_ui.get_node(\"QuitButton\").pressed.connect(_on_game_over_quit)
	
	# Start —Å–∫—Ä–∏—Ç–æ
	pause_ui.visible = false
	game_over_ui.visible = false

# üî• PUBLIC - GameScene –≤–∏–∫–∞
func update_score(new_score: int):
	score = new_score
	score_label.text = \"Score: %d\" % score

func update_time(time_left: float):
	time_label.text = \"Time: %d:%02d\" % [int(time_left/60), int(time_left)%60]

func update_health(new_health: int):
	current_health = new_health
	health_bar.value = current_health

func toggle_pause():
	Engine.time_scale = 1.0 if pause_ui.visible else 0.0
	pause_ui.visible = !pause_ui.visible
	if pause_ui.visible:
		ambience_loop.play()

func show_game_over():
	Engine.time_scale = 0.0
	game_over_ui.visible = true
	game_over_score_label.text = \"Final Score: %d\" % score
	ambience_loop.play()

# üî• PRIVATE - —Å–∞–º–æ –±—É—Ç–æ–Ω–∏
func _on_resume():
	resume_game.emit()  # GameScene —á—É–≤–∞
	toggle_pause()

func _on_pause_retry():
	retry_game.emit()

func _on_pause_quit():
	quit_game.emit()

func _on_game_over_retry():
	retry_game.emit()

func _on_game_over_quit():
	quit_game.emit()
"

